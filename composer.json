{
  "name": "klapuch/storage",
  "type": "library",
  "description": "Easy way how to work with database storage",
  "homepage": "https://github.com/klapuch/Storage",
  "keywords": [
    "pdo",
    "database",
    "storage"
  ],
  "authors": [
    {
      "name": "Dominik Klapuch",
      "email": "klapuchdominik@gmail.com",
      "homepage": "https://www.github.com/klapuch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "nette/tester": "^2.0",
    "mockery/mockery": "^1.0",
    "phing/phing": "^2.16",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-mockery": "^0.12.7",
    "phpstan/phpstan-strict-rules": "^0.12.5",
    "slevomat/coding-standard": "^6.4",
    "consistence/coding-standard": "^3.10",
    "php-parallel-lint/php-parallel-lint": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Klapuch\\Storage\\": [
        "Core/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Klapuch\\Storage\\": [
        "Tests/"
      ]
    }
  }
}
