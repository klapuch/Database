{
  "name": "klapuch/storage",
  "type": "library",
  "description": "Easy way how to work with database storage",
  "homepage": "https://github.com/klapuch/Storage",
  "keywords": [
    "pdo",
    "database",
    "storage"
  ],
  "authors": [
    {
      "name": "Dominik Klapuch",
      "email": "klapuchdominik@gmail.com",
      "homepage": "https://www.github.com/klapuch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-pdo": "*",
    "klapuch/log": "^1.0.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "nette/tester": "^2.0",
    "mockery/mockery": "^0.9.9",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "slevomat/coding-standard": "^4.0",
    "sebastian/phpcpd": "^3.0",
    "phing/phing": "^2.16",
    "consistence/coding-standard": "^2.0",
    "phpstan/phpstan": "^0.8.0"
  },
  "autoload": {
    "psr-4": {
      "Klapuch\\Storage\\": [
        "Core/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Klapuch\\Storage\\": [
        "Tests/"
      ]
    }
  }
}
